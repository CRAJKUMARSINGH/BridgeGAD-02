                                        <div class="form-group">
                                            <label for="SCALE1" class="form-label">
                                                Primary Scale
                                                
                                                <i class="fas fa-info-circle text-info ms-1" 
                                                   data-bs-toggle="tooltip" 
                                                   title="Primary drawing scale factor"></i>
                                            </label>
                                            <input type="number" 
                                                   class="form-control parameter-input" 
                                                   id="SCALE1" 
                                                   name="SCALE1" 
                                                   value="100"
                                                   
